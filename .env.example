# .env.example
# Скопируйте этот файл в .env и заполните реальными значениями

# Токен бота от @BotFather
BOT_TOKEN=your_bot_token_here

# Режим отладки (true/false)
DEBUG=true

# Email для контакта с API Helldivers
CONFIG_EMAIL=your_email@example.com

# Имя бота для идентификации в API
BOT_NAME=DemoBot